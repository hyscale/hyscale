name: rabbitmq

image:
     registry: registry.hub.docker.com  
     name: {{username}}/rabbitmq # replace with your dockerhub username for {{username}}
     tag: 1.0
     buildSpec:
       stackImage: rabbitmq:3.7


ports:
   - port: 4369/tcp
   - port: 5671/tcp
   - port: 5672/tcp
     healthCheck: {}
   - port: 25672/tcp

memory: 512M
